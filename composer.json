{
    "name": "yaskuriyamuri/phpurl",
    "description": "Library untuk generate URL",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "Yaskuriyamuri\\Phpurl\\": "src/",
            "Yaskuriyamuri\\PhpurlTest\\": "test/"
        }
    },
    "authors": [
        {
            "name": "Yaskuri Yamuri",
            "email": "yaskuriyamuri@gmail.com",
            "homepage": "https://dukung.online",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.6||^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "config": {
        "apcu-autoloader": true,
        "classmap-authoritative": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "5.6.40"
        }
    },
    "scripts": {
        "post-update-cmd": "./vendor/bin/phpunit --verbose --configuration phpunit.xml"
    }
}